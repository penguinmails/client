{
  "name": "penguin-mails",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run convex:codegen && next build",
    "start": "next start",
    "lint": "eslint . --cache",
    "typecheck": "npx tsc -p tsconfig.json --noEmit --skipLibCheck",
    "ai": "GEMINI_SYSTEM_MD=1 gemini",
    "test": "jest",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy",
    "convex:dashboard": "convex dashboard",
    "convex:codegen": "npx convex codegen",
    "build:monitored": "node scripts/build-with-monitoring.js",
    "docs:maintenance": "npx tsx scripts/docs-maintenance.ts",
    "db:migrate": "npx tsx scripts/migration/migrations.ts run",
    "db:migrate:rollback": "npx tsx scripts/migration/migrations.ts rollback",
    "db:seed": "npx tsx scripts/migration/seed.ts seed",
    "db:seed:rollback": "npx tsx scripts/migration/seed.ts rollback",
    "test:migration": "jest scripts/__tests__/migration.test.ts",
    "test:middleware": "jest lib/niledb/__tests__/enhanced-middleware.test.ts",
    "test:errors": "jest lib/niledb/__tests__/errors.test.ts",
    "test:error-handling": "npm run test:middleware && npm run test:errors",
    "test:comprehensive": "npx tsx lib/niledb/__tests__/run-comprehensive-tests.ts",
    "test:comprehensive:verbose": "npx tsx lib/niledb/__tests__/run-comprehensive-tests.ts --verbose",
    "test:comprehensive:coverage": "npx tsx lib/niledb/__tests__/run-comprehensive-tests.ts --coverage",
    "test:comprehensive:parallel": "npx tsx lib/niledb/__tests__/run-comprehensive-tests.ts --parallel",
    "test:comprehensive:fast": "npx tsx lib/niledb/__tests__/run-comprehensive-tests.ts --fail-fast",
    "test:auth": "jest lib/niledb/__tests__/auth.test.ts --verbose",
    "test:tenant": "jest lib/niledb/__tests__/tenant.test.ts --verbose",
    "test:company": "jest lib/niledb/__tests__/company.test.ts --verbose",
    "test:api": "jest lib/niledb/__tests__/api-integration.test.ts --verbose",
    "test:ui": "jest components/auth/__tests__/enhanced-auth-system.test.tsx --verbose",
    "test:performance": "jest lib/niledb/__tests__/performance-monitoring.test.ts --verbose",
    "test:integration": "jest lib/niledb/__tests__/comprehensive-test-suite.test.ts --verbose",
    "test:e2e": "jest lib/niledb/__tests__/end-to-end-integration.test.ts --verbose",
    "test:unit": "npm run test:auth && npm run test:tenant && npm run test:company",
    "test:services": "npm run test:unit && npm run test:migration",
    "test:api-routes": "npm run test:api",
    "test:ui-components": "npm run test:ui",
    "test:system": "npm run test:performance && npm run test:integration && npm run test:e2e",
    "test:all": "npm run test:services && npm run test:api-routes && npm run test:ui-components && npm run test:system",
    "test:critical": "npx tsx lib/niledb/__tests__/run-comprehensive-tests.ts --suites=unit-auth,unit-tenant,unit-company,migration-validation,api-integration,comprehensive-integration,end-to-end --fail-fast",
    "test:quick": "npx tsx lib/niledb/__tests__/run-comprehensive-tests.ts --suites=unit-auth,unit-tenant,unit-company --parallel",
    "type-analysis": "npx tsx scripts/type-analysis/cli/analyze-types.ts"
  },
